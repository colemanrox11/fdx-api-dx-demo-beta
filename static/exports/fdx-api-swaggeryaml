                prev:
                  href: /payments?offset=prev-offset-456-abc
                updates:
                  href: /payments?updatedSince=next-update-id-456-krl
              payments:
              - fromAccountId: ACCOUNT-123
                toPayeeId: PAYEE-ABC
                amount: 10.99
                merchantAccountId: MERCHANT-ACCOUNT-ID-0001
                dueDate: 2021-08-17
