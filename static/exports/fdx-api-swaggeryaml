      description: Create a transfer between accounts
      summary: requestAccountTransfer
      tags:
      - Internal Transfers
      operationId: requestAccountTransfer
      deprecated: false
      produces:
      - application/json
      consumes:
      - application/json
      parameters:
      - name: Authorization
        in: header
        required: false
        default: Bearer {token}
        type: string
      - name: x-fapi-interaction-id
        in: header
        required: true
        type: string
        format: uuid
        description: Unique identifier for this interaction
      - name: idempotency-key
        in: header
        required: true
        type: string
        maxLength: 256
        description: Used to de-duplicate requests
      - name: Content-Type
        in: header
        required: false
        enum:
        - application/json
        type: string
        description: ''
      - name: FDX-API-Actor-Type
        in: header
        required: false
        enum:
        - BATCH
        - USER
        type: string
        description: Identifies whether the customer is present (USER) or it is a BATCH operation
      - name: body
        in: body
        required: false
        description: Data of the transfer request
        schema:
          $ref: '#/definitions/TransferforCreateentity1'
      responses:
        '200':
          description: Initial status of the requested transfer
          schema:
            $ref: '#/definitions/Transferentity'
          examples:
            application/json:
              transferId: MY-TRANSFER-ID
              fromAccountId: ACCOUNT-123
              toAccountId: ACCOUNT-456
              amount: 100
              memo: FDX Transfer Example
              paymentDetails:
                principalAmount: 75
                interestAmount: 10
                insuranceAmount: 5
                escrowAmount: 5
                pmiAmount: 5
                feesAmount: 0
              referenceId: FI-TRANSFER-ID
              status: SCHEDULED
              transferTime: 2021-03-14T13:15:30.751Z
          headers:
            x-fapi-interaction-id:
              description: Unique identifier for this interaction
              type: string
        '400':
          description: Source account does not have sufficient funds
          schema:
            $ref: '#/definitions/Error1'
          examples:
            application/json:
              code: '910'
              message: Insufficient funds
              debugMessage: Provider custom developer-level error details for troubleshooting
          headers:
            x-fapi-interaction-id:
              description: Unique identifier for this interaction
              type: string
        '401':
